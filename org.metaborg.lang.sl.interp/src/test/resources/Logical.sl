function main() {
  println(true == true);
  println(false == false);
  println(42 == 42);
  println(43 == 99);
  println(42 != 42);
  println("42" == "42");
  println(42 != "42");
  println(true && true);
  println(true && false);
  println(true && false);
  println(false && false);
  println(true || true);
  println(true || false);
  println(false || true);
  println(false || false);
  println(!true);
  println(!false);
}